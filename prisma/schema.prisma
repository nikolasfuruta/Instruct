generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FeriadoNacional {
  id          Int     @id @default(autoincrement())
  dataFeriado String
  nomeFeriado String
  tipoMovel   Boolean
}

model FeriadoEstadual {
  id          Int     @id @default(autoincrement())
  dataFeriado String
  nomeFeriado String
  cod         Int
  tipoMovel   Boolean
}

model FeriadoMunicipal {
  id          Int     @id @default(autoincrement())
  dataFeriado String
  nomeFeriado String
  cod         Int
  tipoMovel   Boolean
}
